{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "/backend/src/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "BACKEND_URL": "https://derma-dao-junquan2.vercel.app/api"
  }
}
